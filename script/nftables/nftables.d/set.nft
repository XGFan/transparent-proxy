set proxy_dst {
    type ipv4_addr
    flags interval
    auto-merge
}

set proxy_src {
    type ipv4_addr
    flags interval
    auto-merge
    elements = { 192.168.2.65 }
}

set direct_src {
    type ipv4_addr
    flags interval
    auto-merge
    elements = { 192.168.2.60, 192.168.2.104, 192.168.2.151 }
}

set direct_dst {
    type ipv4_addr
    flags interval
    auto-merge
